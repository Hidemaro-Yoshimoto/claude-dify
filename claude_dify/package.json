{
  "name": "playwright-analyzer",
  "version": "1.0.0",
  "description": "Playwright web analysis API with TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "playwright": "^1.40.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
